{
    "name": "Blockdiag",
    "version": "1.0.1",
    "author": [
        "David Raison",
        "Gilles R. Magalhaes",
        "Kazunori Kojima"
    ],
    "url": "https://github.com/tentwentyfour/blockdiag-mediawiki-extension",
    "descriptionmsg": "blockdiag-desc",
    "license-name": "Apache-2.0",
    "type": "parserhook",
    "requires": {
        "MediaWiki": ">= 1.25.0"
    },
    "LogTypes": [
            "renameuser"
    ],
    "MessagesDirs": {
            "Blockdiag": [
                    "i18n"
            ]
    },
    "AutoloadClasses": {
        "Blockdiag": "Blockdiag.php"
    },
    "Hooks": {
        "ParserFirstCallInit" : [
            "Blockdiag::parserInit"
        ]
    },
    "config": {
        "BlockdiagPath": "/usr/local/bin"
    },
    "manifest_version": 1
}
